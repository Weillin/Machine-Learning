    "import torch\n",
    "\n",
    "x, w_xh = torch.normal(0, 1, (3, 1)), torch.normal(0, 1, (1, 4))\n",
    "h, w_hh = torch.normal(0, 1, (3, 4)), torch.normal(0, 1, (4, 4))\n",
    "torch.matmul(x, w_xh) + torch.matmul(h, w_hh)"
   ]

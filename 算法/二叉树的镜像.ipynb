   "source": [
    "## 操作给定的二叉树，将其变换为源二叉树的镜像"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "5990afb1",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def Mirror(self, pRoot):\n",
    "        if pRoot == None:\n",
    "            return None\n",
    "\n",
    "        pRoot.left, pRoot.right = pRoot.right, pRoot.left\n",
    "\n",
    "        self.Mirror(pRoot.left)\n",
    "        self.Mirror(pRoot.right)\n",
    "\n",
    "        return pRoot"
   ]
  }
